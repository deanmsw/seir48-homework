<%= form_for @pet do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :DOB %>
  <%= f.text_field :dob %>
  <%= f.label :image %>
  <%= f.url_field :image %>
  <%= f.label :breeder_id %>
  <%= f.select :breeder_id, Breeder.pluck(:name, :id), :include_blank => true %>
  <%= f.submit %>
<% end %>